
#INCLUDE "<inc>/Util.hjk"

#MACRO INDEX2 ( M, i, j ) [ M[(i << 4) + j] ]

FN ArrEquals(
    IN a : ^UWORD,
    IN b : ^UWORD,
) : UWORD
    i := 0
    j := 0
    WHILE i < 16 DO
        WHILE j < 16 DO
            IF INDEX2(a,i,j) != INDEX2(b,i,j) THEN 
                RETURN 0
            END
            j += 1
        END
        i += 1
    END
    RETURN 1
END

