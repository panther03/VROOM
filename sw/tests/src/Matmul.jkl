FN TestMain () : UWORD    
    i := 0
    j := 0
    WHILE i < 16 DO
        j = 0
        WHILE j < 16 DO
            INDEX2(A,i,j) = i+1
            INDEX2(B,i,j) = i + j
            j += 1
        END
        i += 1
    END

    sum : UWORD
    i = 0
    k := 0
    WHILE i < 16 DO
        j = 0
        WHILE j < 16 DO
            sum = 0
            k = 0
            WHILE k < 16 DO
                sum += SoftMul(INDEX2(A,i,k), INDEX2(B,k,j))
                k += 1
            END
            INDEX2(C,i,j) = sum
            j += 1
        END
        i += 1
    END

    RETURN NOT ArrEquals(&C[0], &Expected[0])
END

